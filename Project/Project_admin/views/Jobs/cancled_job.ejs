        <li class="breadcrumb-item active">Rejected Jobs</li>
    </ol>
</div>

<%- contentFor('body') %>


                        <div class="row">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-body">

                                        <h4 class="mt-0 header-title">Rejected Jobs</h4>
                                       

                                        <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                            <thead>
                                            <tr>
                                                <th>S.No</th>
                                                <th>User Name</th>
                                                <th>Job Title</th>
                                                <th>Price</th>
                                                
                                                <th>Date</th>
                                                <th>Time</th>
                                                <th>Address</th>
                                                <th>Status</th>

                                              

                                               
                                            </tr>
                                            </thead>


                                            <tbody>
                                                
                                                    <% result.forEach(function(item,i){ %>

                                                        <tr>
                                                            <td><%= i+1 %></td>
                                                            <td><%= item.name%></td>
                                                            <td><%= item.job_title%></td>
                                                            <td><%= item.price%></td>
                                                            <td><%= item.date%></td>
                                                            <td><%= item.time%></td>
                                                            <td><%= item.address%></td>
                                                            
                                                         
                                                            
                                                          
                                                            <% if(item.status==3){%>
                                                                <td><button class="button">complete</button></td>
                                                            <%}%>
            
                                                            
                                                            
                                                        </tr>
                                                     
                                                      
                                                      
                                                       
                                                       
                                                       <% }) %>
                                               
                                                
                                          
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div> <!-- end col -->
                        </div> <!-- end row -->

